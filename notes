
BowItem:
    type:
        Riser:
            props:
            riserName, riserLength, riserMaterial, riserColour, riserHand, riserWeight, riserPocket

        Limbs:
            props:
            limbName, limbLength, limbMaterial, limbDrawWeight, limbPocket

        Takedown:
            type:
                recurveBow, longBow
            props:
            bowName, bowHand, bowLength, bowDrawWeight, bowColour
        
        Onepiece:
            type:
                recurve, longbow
            props:
            bowName, bowLength, bowDrawWeight, bowHand
        
        Compound:
            type:
                target, hunting
            props:
            bowName, modelYear, bowHand, bowLength, bowColour, bowLetOff, bowWeight, bowDrawWeight

    props:
        manufacturer, archeryStyle, price



